@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix saref: <http://uri.etsi.org/m2m/saref#> .

_:V102 rdf:type ec:C_AKE773003-gen .
_:S111 rdf:type ec:C_AKE672002-gen .
_:T101 rdf:type ec:C_AKE989002-gen .

# TODO refine
_:V102Status a sosa:ObservableProperty, sosa:ActuatableProperty .
_:S111OverflowStatus a sosa:ObservableProperty, sosa:ActuatableProperty .

_:V102 rdf:type td:Thing ;
    td:name "Valve" ;
    sosa:madeActuation [
        sosa:usedProcedure [
            rdf:type td:Action ;
            ssn:implementedBy _:V102 ;
            td:name "OpenPneumaticValve" ;
            td:form [ td:href <coap://193.168.2.199:5683/open> ]
        ] ;
        sosa:actsOnProperty _:V102Status
    ], [
        sosa:usedProcedure [
            rdf:type td:Action ;
            ssn:implementedBy _:V102 ;
            td:name "ClosePneumaticValve" ;
            td:form [ td:href <coap://193.168.2.199:5683/close> ]
        ] ;
        sosa:actsOnProperty _:V102Status
    ] ;
    sosa:madeObservation [
        sosa:usedProcedure [
            rdf:type td:Property ;
            ssn:implementedBy _:V102 ;
            td:name "Status" ;
            td:form [ td:href <coap://193.168.2.199:5683/status> ]
        ] ;
        sosa:observedProperty _:V102Status
    ] .
    
_:S111 rdf:type td:Thing, sosa:Sensor ;
    td:name "LevelSwitchS111" ;
    sosa:isHostedBy _:T101 ;
    sosa:madeObservation [
        sosa:usedProcedure [
            rdf:type td:Property ;
            ssn:implementedBy _:S111 ;
            td:name "tank101OverflowStatus" ;
            td:form [ td:href <coap://192.168.2.199:5683/LevelSwitchS111/tank101OverflowStatus> ]
        ] ;
        sosa:observedProperty _:S111OverflowStatus
    ] .