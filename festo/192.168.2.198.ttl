_:P101Status a sosa:ObservableProperty, sosa:ActuatableProperty .
_:V106Value a sosa:ObservableProperty, sosa:ActuatableProperty .
_:B102Flow a sosa:ObservableProperty, sosa:ActuatableProperty .

_:P101 rdf:type td:Thing ;
    td:name "Pump" ;
    sosa:madeActuation [
        sosa:usedProcedure [
            rdf:type td:Action ;
            ssn:implementedBy _:P101 ;
            td:name "On" ;
            td:form [ td:href <coap://192.168.2.198:5683/PumpP101/on> ]
        ] ;
        sosa:actsOnProperty _:P101Status
    ], [
        sosa:usedProcedure [
            rdf:type td:Action ;
            ssn:implementedBy _:P101 ;
            td:name "Off" ;
            td:form [ td:href <coap://192.168.2.198:5683/PumpP101/off> ]
        ] ;
        sosa:actsOnProperty _:P101Status
    ] ;
    sosa:madeObservation [
        sosa:usedProcedure [
            rdf:type td:Property ;
            ssn:implementedBy _:P101 ;
            td:name "Status" ;
            td:form [ td:href <coap://192.168.2.198:5683/PumpP101/status> ]
        ] ;
        sosa:observedProperty _:P101Status
    ] ;
    sosa:madeObservation [
        sosa:usedProcedure [
            rdf:type td:Event ;
            ssn:implementedBy _:P101 ;
            td:name "criticalCondition" ;
            td:form [ td:href <coap://192.168.2.198:5683/PumpP101/criticalCondition> ]
        ] ;
        sosa:observedProperty _:P101Status # TODO not exactly
    ] .

_:V106 rdf:type td:Thing ;
    td:name "ProportionalValve" ;
